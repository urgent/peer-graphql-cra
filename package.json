{
  "name": "peer-graphql",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-relay": "^7.0.12",
    "graphql-scalars": "^1.4.0",
    "isomorphic-ws": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-relay": "^0.0.0-experimental-183bdd28",
    "react-scripts": "3.4.3",
    "relay-runtime": "^10.0.1",
    "source-map-explorer": "^2.5.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "yarn run generate && yarn run relay && react-scripts start",
    "build": "yarn run relay && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./src/features/peer-graphql/graphql/schema.graphql --extensions js jsx tsx graphql --language typescript",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/relay-runtime": "^10.0.4",
    "@types/webpack-env": "^1.15.3",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-plugin-relay": "^10.0.1",
    "fast-check": "^2.3.0",
    "fp-ts": "^2.8.3",
    "graphql": "^15.3.0",
    "io-ts": "^2.2.10",
    "relay-compiler": "^10.0.1",
    "relay-compiler-language-typescript": "^13.0.1",
    "relay-config": "^10.0.1",
    "typescript": "^4.0.3",
    "webpack-cli": "^3.3.12",
    "yarn": "^1.22.5"
  },
  "resolutions": {
    "react-scripts/@typescript-eslint/parser": "^4.2.0",
    "react-scripts/@typescript-eslint/eslint-plugin": "^4.2.0"
  }
}
